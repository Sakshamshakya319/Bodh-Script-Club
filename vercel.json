{
  "version": 2,
  "routes": [
    {
      "src": "/api/create-admin",
      "dest": "/api/create-admin.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/diagnostic",
      "dest": "/api/diagnostic.js"
    },
    {
      "src": "/api/payment/(.*)",
      "dest": "/api/payment.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
